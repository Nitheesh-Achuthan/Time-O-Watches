<body>
  <header>
    <!-- Header Start -->
    <div class="header-area">
        <div class="main-header header-sticky">
            <div class="container-fluid">
                <div class="menu-wrapper">
                    <!-- Logo -->
                    <div class="logo">
                        <a href="index.html"><img style="padding-left:3rem ;" src="../assets/img/logo/logo.svg" alt=""></a>
                    </div>
                    <!-- Main-menu -->
                    <div class="main-menu d-none d-lg-block">
                        
                    </div>
                    <!-- Header Right -->
                    <div class="header-right">
                        <ul>
                            
                            <li><a href="/"><span class="">Home</span></a></li>
                            <li><a href="/signup"><span class="">Sign Up</span></a></li>
                            
                        </ul>
                    </div>
                </div>
                <!-- Mobile Menu -->
