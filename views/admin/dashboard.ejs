<script>
  
var xValues = ['Ordered','Delivered','Shiped','Canceled'];
var yValues = ['<%=orderedCount%>','<%=deliveredCount%>','<%=shipedCount%>','<%=canceledCount%>'];

new Chart("myChartTwo", {
  type: "line",
  data: {
    labels: xValues,
    datasets: [{
      label:'Order Status',
      fill: false,
      lineTension: 0,
      backgroundColor: "rgba(0,0,255,1.0)",
      borderColor: "rgba(0,0,0,0)",
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    scales: {
      yAxes: [{ticks: {min: 0, max:10}}],
    }
  }
});




var xValues = ["Active", "Blocked"];
var yValues = ['<%=activeCount%>','<%=blockedCount%>'];
var barColors = [ "green","brown"];

new Chart("myChartThree", {
  type: "bar",
  data: {
    labels: xValues,
    datasets: [{
      label:'User Status',
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "Users"
    }
  }
});


var xValues = ["Online", "COD"];
var yValues = ['<%=onlineCount%>','<%=codCount%>'];
var barColors = [
  // "#b91d47",
  "#00aba9",
  // "#2b5797",
  "#e8c3b9"
  // "#1e7145"
];

new Chart("myChart", {
  type: "pie",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    title: {
      display: true,
      text: "Payments"
    }
  }
});

</script>


